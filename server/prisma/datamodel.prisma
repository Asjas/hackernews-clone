type Link @db(name: "link") {
  id: ID! @id
  description: String!
  url: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  postedBy: User
  votes: [Vote!]!
}

type User @db(name: "user") {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  links: [Link!]!
  votes: [Vote!]!
}

type Vote @db(name: "vote") {
  id: ID! @id
  link: Link!
  user: User!
}
